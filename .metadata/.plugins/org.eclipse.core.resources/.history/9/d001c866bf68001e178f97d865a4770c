package com.lao;

import java.lang.reflect.Constructor;

public class Example3Contructor {
		
	String name;
	String type;
	
	Example3Contructor(){
		String Name = name;
		String Type = type;
		System.out.println(Name+Type);
		
	}
	
	public static void main(String[] args) {
		Example3Contructor contructor = new Example3Contructor();
		//default constructor
		System.out.println(contructor.name+contructor.type);
		
		//non parameterized constructor
		Example3Contructor contructor2 = new Example3Contructor();
		System.out.println(contructor2.);
		
	}
}
